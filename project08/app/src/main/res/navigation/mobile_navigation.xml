<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.project07.ui.home.HomeFragment"
        android:label="首页"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.project07.ui.dashboard.DashboardFragment"
        android:label="车里程"
        tools:layout="@layout/fragment_dashboard" />


    <fragment
        android:id="@+id/smart"
        android:name="com.example.project07.ui.home.SmartFragment"
        android:label="@string/title_notifications"
        tools:layout="智慧社区" />
    <fragment
        android:id="@+id/carlive"
        android:name="com.example.project07.ui.home.CarLiveFragment"
        android:label="车生活"
        tools:layout="@layout/fragment_car_live" />
    <fragment
        android:id="@+id/person"
        android:name="com.example.project07.ui.notifications.NotificationsFragment"
        android:label="我的"
        tools:layout="@layout/fragment_notifications" />
</navigation>